{
  "name": "larvitammail",
  "version": "0.4.132",
  "description": "Mailing framework for larvit AM project",
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "lint": "eslint *.js test/**/*.js",
