  },
  "homepage": "https://github.com/larvit/larvitammail#readme",
  "devDependencies": {
    "eslint": "8.57.1",
    "mocha": "11.7.2",
    "nodemailer-stub-transport": "1.1.0",
    "nyc": "17.1.0",
    "sinon": "21.0.0"
  },
  "dependencies": {
    "async": "3.2.6",
    "larvitamintercom": "1.0.18",
    "larvitmail": "3.0.140",
    "larvitutils": "5.1.161",
    "lodash": "4.17.21"
